<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/onBoardingFragment1">

    <fragment
        android:id="@+id/registrationFragment"
        android:name="com.example.m5hw2.RegistrationFragment"
        android:label="fragment_registration"
        tools:layout="@layout/fragment_registration" >
        <action
            android:id="@+id/action_registrationFragment_to_loveResultFragment"
            app:destination="@id/loveResultFragment" />
    </fragment>
    <fragment
        android:id="@+id/loveResultFragment"
        android:name="com.example.m5hw2.LoveResultFragment"
        android:label="fragment_love_result"
        tools:layout="@layout/fragment_love_result" >

    <argument
        android:name="percentage"
        app:argType="string" />

    </fragment>
    <fragment
        android:id="@+id/onBoardingFragment1"
        android:name="com.example.m5hw2.onboard.OnBoardingFragment1"
        android:label="fragment_on_boarding1"
        tools:layout="@layout/fragment_on_boarding1" >
        <action
            android:id="@+id/action_onBoardingFragment1_to_onBoardingFragment2"
            app:destination="@id/onBoardingFragment2" />
    </fragment>
    <fragment
        android:id="@+id/onBoardingFragment2"
        android:name="com.example.m5hw2.onboard.OnBoardingFragment2"
        android:label="fragment_on_boarding2"
        tools:layout="@layout/fragment_on_boarding2" >
        <action
            android:id="@+id/action_onBoardingFragment2_to_onBoardingFragment3"
            app:destination="@id/onBoardingFragment3" />
    </fragment>
    <fragment
        android:id="@+id/onBoardingFragment3"
        android:name="com.example.m5hw2.onboard.OnBoardingFragment3"
        android:label="fragment_on_boarding3"
        tools:layout="@layout/fragment_on_boarding3" />

</navigation>